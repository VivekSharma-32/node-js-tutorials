<%- include('header') %>
    <div class="row mb-5">
        <div class="col-md-6">
            <h1>
                <%= title %>
            </h1>
            <% if(message) { %>
                <p>
                    <%= message %>
                </p>
                <% } else { %>
                    <span>No additional information available.</span>
                    <% } %>
                        <table class="table">
                            <% items.forEach((item)=>{ %>
                                <tr>
                                    <td>
                                        <%= item.id _%>
                                    </td>
                                    <td>
                                        <%= item.name _%>
                                    </td>
                                    <td>
                                        <%= item.age _%>
                                    </td>
                                    <% }) %>
                                </tr>
                        </table>
        </div>
    </div>

    <%- include('footer') %>